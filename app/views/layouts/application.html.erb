<!DOCTYPE html>
<html>
	<head>
	  <title>Poetry</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>
	</head>
	<body>

		<div id="wrapper">
		 
			<!-- Structure source: "https://www.railstutorial.org/book/filling_in_the_layout" -->
			<%= render 'layouts/header' %>


			<div class="container">
				<%= yield %>

				<%= render 'layouts/footer' %>
			</div> 	
		</div>	
	</body>
</html>
